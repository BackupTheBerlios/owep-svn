<!DOCTYPE databases PUBLIC "-//EXOLAB/Castor JDO Configuration DTD Version 1.0//EN" "http://castor.exolab.org/jdo-conf.dtd">

<jdo-conf>
  <database name="owep" engine="mysql">
    <driver class-name="com.mysql.jdbc.Driver" url="jdbc:mysql://localhost:3306/owep">
      <param name="user" value="root"/>
      <param name="password" value="*****"/>
    </driver>
    <mapping href="Owep-mapping.xml"/>
  </database>
  <transaction-demarcation mode="local"/>
</jdo-conf>
